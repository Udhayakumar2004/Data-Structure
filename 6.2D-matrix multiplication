#include<stdio.h>

int main()

{

	int i,j,a[12][12],a2[12][12],n;	printf("enter the size:");

	scanf("%d",&n);

	

	for(i=0;i<n;i++)

	{         

		for(j=0;j<n;j++)

		{         

			printf("enter the element:");

			scanf("%d",&a[i][j]);

		}

	}

	

	printf("next array\n");

	for(i=0;i<n;i++)

	{         

		for(j=0;j<n;j++)

		{         

			printf("enter the element:");

			scanf("%d",&a2[i][j]);

		}

	}

		for(i=0;i<n;i++)

	{          

		for(j=0;j<n;j++)

		{         

			

			printf("%d\t",a[i][j]*a2[i][j]);

			

	}

	printf("\n");

		}

		

	return 0;

}
